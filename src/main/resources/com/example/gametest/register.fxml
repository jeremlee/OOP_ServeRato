<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="800.0" prefWidth="1199.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gametest.RegisterController">
   <children>
      <ImageView fitHeight="800.0" fitWidth="1199.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@login_pic.png" />
         </image>
      </ImageView>
      <VBox alignment="CENTER" layoutX="475.0" layoutY="408.0" prefHeight="258.0" prefWidth="249.0" spacing="10.0">
         <children>
            <Label text="Username">
               <font>
                  <Font name="Montserrat Regular" size="14.0" />
               </font></Label>
            <TextField fx:id="txtfUsername" />
            <Label text="Password">
               <font>
                  <Font name="Montserrat Regular" size="14.0" />
               </font>
            </Label>
            <PasswordField fx:id="txtfPassword" onKeyPressed="#onRegisterEnter" />
            <ImageView fx:id="imgRegister" fitHeight="150.0" fitWidth="200.0" onMouseClicked="#onRegisterClick" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@signup.png" />
               </image>
            </ImageView>
            <ImageView fx:id="imgLogin" fitHeight="150.0" fitWidth="200.0" onMouseClicked="#toLogin" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@login_button.png" />
               </image>
            </ImageView>
            <Label fx:id="lblErrorMsg" textFill="WHITE">
               <font>
                  <Font size="16.0" />
               </font></Label>
         </children>
         <effect>
            <DropShadow />
         </effect>
      </VBox>
   </children>
</AnchorPane>
