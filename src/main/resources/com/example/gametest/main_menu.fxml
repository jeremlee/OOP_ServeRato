<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gametest.MainMenuController">
   <children>
      <ImageView fitHeight="800.0" fitWidth="1200.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@login_pic.png" />
         </image>
      </ImageView>
      <ImageView fx:id="imgPlay" fitHeight="150.0" fitWidth="200.0" layoutX="500.0" layoutY="360.0" onMouseClicked="#onPlayClick" onMouseEntered="#onPlayerHoverIn" onMouseExited="#onPlayerHoverOut" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@play.png" />
         </image>
      </ImageView>
      <ImageView fx:id="imgLeaderboards" fitHeight="150.0" fitWidth="200.0" layoutX="500.0" layoutY="434.0" onMouseClicked="#onLeaderboardsClick" onMouseEntered="#onLeaderboardsHoverIn" onMouseExited="#onLeaderboardsHoverOut" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@leaderboards.png" />
         </image>
      </ImageView>
      <ImageView fx:id="imgExit" fitHeight="150.0" fitWidth="200.0" layoutX="500.0" layoutY="594.0" onMouseClicked="#onExitClick" onMouseEntered="#onExitHoverIn" onMouseExited="#onExitHoverOut" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@exit.png" />
         </image>
      </ImageView>
      <ImageView fx:id="imgLogout" fitHeight="150.0" fitWidth="200.0" layoutX="500.0" layoutY="514.0" onMouseClicked="#onLogoutClick" onMouseEntered="#onLogoutHoverIn" onMouseExited="#onLogoutHoverOut" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@logout.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
