<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="1200f" prefWidth="800f" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gametest.LoginRegisterController">
   <children>
      <GridPane alignment="CENTER" layoutX="276.0" layoutY="273.0" prefHeight="121.0" prefWidth="397.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="118.0" minWidth="0.0" prefWidth="0.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="256.0" minWidth="10.0" prefWidth="256.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <TextField fx:id="txtfUsername" GridPane.columnIndex="1" />
            <PasswordField fx:id="txtfPassword" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Button fx:id="btnRegister" contentDisplay="CENTER" mnemonicParsing="false" onAction="#onRegisterClick" text="Register" GridPane.rowIndex="2" />
            <Button fx:id="btnLogin" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#onLoginClick" text="Login" textAlignment="CENTER" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label text="Username" />
            <Label text="Password" GridPane.rowIndex="1" />
            <Label fx:id="lblErrorMsg" GridPane.columnIndex="1" GridPane.rowIndex="3" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
